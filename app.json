{
  "expo": {
    "name": "Happy Journey Ai",
    "slug": "happy-journey-",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/app-image/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ponyweb.happy.journey",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/app-image/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/app-image/icon.png"
    },
    "splash": {
      "image": "./src/assets/images/app-image/icon.png",
      "backgroundColor": "#20C997",
      "resizeMode": "contain"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/app-image/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#20C997"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0bd43b24-1036-48cc-8976-af6e4bc19e32"
      }
    },
    "owner": "pony-web",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0bd43b24-1036-48cc-8976-af6e4bc19e32"
    }
  }
}
